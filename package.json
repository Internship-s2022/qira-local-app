{
  "name": "react-app-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "wdio-test": "npx wdio run ./wdio.conf.js",
    "type-check": "tsc --noEmit",
    "lint": "eslint \"./**/*.{ts,tsx,test.ts,test.tsx}\" && stylelint \"./src/**/*.css\"",
    "format": "prettier --write './**/*.{ts,tsx,test.ts,test.tsx}' --config ./.prettierrc",
    "lint:fix": "eslint --fix \"./**/*.{ts,tsx,test.ts,test.tsx}\" && stylelint \"./src/**/*.css\" --fix && yarn format",
    "pre-commit": "yarn type-check && yarn lint && yarn test --watchAll=false",
    "set:dev": "vercel env pull .env.dev",
    "prepare": "husky install",
    "pretest": "rimraf allure-results && rimraf allure-report && rimraf test-screenshots && rimraf wdio-logs",
    "allure-report": "yarn allure generate --clean allure-results && yarn allure open"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "stylelint"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.19.4",
    "@babel/register": "^7.18.9",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "@wdio/allure-reporter": "7.20.0",
    "@wdio/cli": "^7.25.1",
    "@wdio/jasmine-framework": "^7.25.1",
    "@wdio/local-runner": "^7.25.1",
    "@wdio/spec-reporter": "^7.25.1",
    "allure-commandline": "2.17.2",
    "chromedriver": "106.0.1",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^8.0.0",
    "prettier": "^2.7.1",
    "stylelint": "^14.11.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard": "^28.0.0",
    "stylelint-prettier": "^2.0.0",
    "vercel": "^28.1.0",
    "wdio-chromedriver-service": "^8.0.0",
    "wdio-wait-for": "^3.0.0"
  },
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@hookform/resolvers": "^2.9.8",
    "@material-ui/styles": "^4.11.5",
    "@mui/icons-material": "^5.10.6",
    "@mui/material": "^5.10.6",
    "@mui/styled-engine": "^5.10.7",
    "@mui/x-date-pickers": "^5.0.8",
    "@redux-devtools/core": "^3.13.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.49",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "axios": "^0.27.2",
    "date-fns": "^2.29.3",
    "firebase": "^9.10.0",
    "joi": "^17.6.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.36.0",
    "react-redux": "^8.0.4",
    "react-router-dom": "^6.4.1",
    "react-scripts": "5.0.1",
    "redux": "^4.2.0",
    "redux-devtools-extension": "^2.13.9",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.4.1",
    "reselect": "^4.1.6",
    "typesafe-actions": "^5.1.0",
    "typescript": "^4.7.4",
    "web-vitals": "^2.1.4"
  }
}
